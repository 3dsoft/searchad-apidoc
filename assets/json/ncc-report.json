      "get": {
        "tags": ["advanced-report-controller"],
        "summary": "NCC 보고서 지표 조회",
        "description": "NCC 보고서 지표를 제공하는 API",
        "operationId": "getStatUsingGET",
        "consumes": ["application/json"],
        "produces": ["*/*"],
